{
  "name": "ukshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn lint && next build",
    "start": "next start",
    "lint": "tsc --noEmit && yarn lint:js && yarn lint:css",
    "lint:js": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,scss,md}\"",
    "posttest": "yarn format",
    "pretest": "yarn lint",
    "lint:css": "stylelint '**/*.*css' --fix",
    "prepare": "yarn openapi-typescript https://app.swaggerhub.com/apiproxy/registry/I425/ReactJS/1.0.0 --prettier-config --output src/types/Api.d.ts && prettier src/types/Api.d.ts --write",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --"
  },
  "dependencies": {
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.4.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.1.1",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "openapi-typescript": "^5.4.1",
    "postcss": "^8.4.17",
    "postcss-scss": "^4.0.5",
    "prettier": ">=2",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-strict-value": "^1.9.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typescript": "4.8.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./test/jestsetup.ts"
    ]
  }
}
